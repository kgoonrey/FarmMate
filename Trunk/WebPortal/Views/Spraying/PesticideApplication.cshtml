@model PesticideApplicationHeader
@using WebPortal.Models
@using WebPortal.Code

@{
    ViewBag.Title = "Pesticide Application";
}

<form id="uiForm" method="post" name="uiForm">
    <h1>Pesticide Application</h1>
    <br />

    <div></div>

    <div id="tabstrip">
        <ul>
            <li class="k-state-active">
                Details
            </li>
            <li>
                Health And Safety
            </li>
            <li>
                Products
            </li>
            <li>
                Required
            </li>
            <li>
                Optional
            </li>
            <li>
                Confirm
            </li>
        </ul>
        <div>
            <label for="company">Company</label>
            @using (Html.BeginForm())
            {
                <div class="form-group">
                    @Html.DropDownListFor(model => model.TradingEntity, (IEnumerable<SelectListItem>)ViewBag.TradingEntitys, "Select Company...", new { @class = "form-control", Id = "company", style = "width: 100%;" })
                </div>
            }
            <label for="employee">Employee</label>
            @using (Html.BeginForm())
            {
                <div class="form-group">
                    @Html.DropDownListFor(model => model.Employee, (IEnumerable<SelectListItem>)ViewBag.Employees, "Select Employee...", new { @class = "form-control", Id = "employee", style = "width: 100%;" })
                </div>
            }

            <label for="uiFarmId">Farm Id No.</label>
            <div class="form-group">
                <input id="uiFarmId" asp-for="FarmId" class="k-textbox" style="width:100%" />
            </div>

            <label for="uiBlockNumbers">Block No/s</label>
            <div class="form-group">
                <input id="uiBlockNumbers" asp-for="BlockNumbers" class="k-textbox" style="width:100%" />
            </div>

            <label for="uiAreaTreated">Area Treated (ha)</label>
            <div class="form-group">
                <input id="uiAreaTreated" asp-for="AreaTreated" style="width:100%" />
            </div>

            <label for="uiDateApplied">Date Applied</label>
            <div class="form-group">
                <input id="uiDateApplied" asp-for="DateApplied" style="width:100%" />
            </div>

        </div>
        <div>

            <input asp-for="ProductLabelReadAndUnderstood" type="checkbox">
            <label asp-for="ProductLabelReadAndUnderstood" style="margin-right:10px">Product Label Read & Understood</label>
            <br />
            <input asp-for="MSDSReadAndUnderstood" type="checkbox">
            <label asp-for="MSDSReadAndUnderstood" style="margin-right:10px">MSDS Read & Understood</label>
            <br />
            <input asp-for="RiskAssessmentUndertaken" type="checkbox">
            <label asp-for="RiskAssessmentUndertaken" style="margin-right:10px">Risk Assessment Undertaken</label>
            <br /><br />
            <label>Neighbours Notified</label>
            <br />
            <input asp-for="NeighboursNotifiedVerbal" type="checkbox">
            <label asp-for="NeighboursNotifiedVerbal" style="margin-right:10px">Verbal</label>
            <input asp-for="NeighboursNotifiedWritten" type="checkbox">
            <label asp-for="NeighboursNotifiedWritten" style="margin-right:10px">Written</label>
            <br /><br />
            <label>Personal protective equipment for application and/or mixing</label>
            <br />
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-3, col-md-3, col-sm-3">
                        <img onclick="ImageClick(uiACCab)" src="data:image/png;base64," />
                        <br />
                        <input id="uiACCab" asp-for="ACCab" type="checkbox">
                        <label asp-for="ACCab" style="margin-right:10px">AC Cab</label>
                    </div>
                    <div class="col-lg-3, col-md-3, col-sm-3">
                        <img onclick="ImageClick(uiOveralls)" src="data:image/png;base64," />
                        <br />
                        <input id="uiOveralls" asp-for="Overalls" type="checkbox">
                        <label asp-for="Overalls" style="margin-right:10px">Overalls</label>
                    </div>
                    <div class="col-lg-3, col-md-3, col-sm-3">
                        <img onclick="ImageClick(uiGoggles)" src="data:image/png;base64," />
                        <br />
                        <input id="uiGoggles" asp-for="Goggles" type="checkbox">
                        <label asp-for="Goggles" style="margin-right:10px">Goggles</label>
                    </div>
                    <div class="col-lg-3, col-md-3, col-sm-3">
                        <img onclick="ImageClick(uiRespirator)" src="data:image/png;base64," />
                        <br />
                        <input id="uiRespirator" asp-for="Respirator" type="checkbox">
                        <label asp-for="Respirator" style="margin-right:10px">Respirator</label>
                    </div>
                    <div class="col-lg-3, col-md-3, col-sm-3">
                        <img onclick="ImageClick(uiHat)" src="data:image/png;base64," />
                        <br />
                        <input id="uiHat" asp-for="Hat" type="checkbox">
                        <label asp-for="Hat" style="margin-right:10px">Hat</label>
                    </div>
                    <div class="col-lg-3, col-md-3, col-sm-3">
                        <img onclick="ImageClick(uiApron)" src="data:image/png;base64," />
                        <br />
                        <input id="uiApron" asp-for="Apron" type="checkbox">
                        <label asp-for="Apron" style="margin-right:10px">Apron</label>
                    </div>
                    <div class="col-lg-3, col-md-3, col-sm-3">
                        <img onclick="ImageClick(uiBoots)" src="data:image/png;base64," />
                        <br />
                        <input id="uiBoots" asp-for="Boots" type="checkbox">
                        <label asp-for="Boots" style="margin-right:10px">Boots</label>
                    </div>
                    <div class="col-lg-3, col-md-3, col-sm-3">
                        <img onclick="ImageClick(uiGloves)" src="data:image/png;base64," />
                        <br />
                        <input id="uiGloves" asp-for="Gloves" type="checkbox">
                        <label asp-for="Gloves" style="margin-right:10px">Gloves</label>
                    </div>
                </div>
            </div>
        </div>
        <div>
            <div class="panel-body">
                <div class="btn-group">
                    <a id="createAddLineModal" class="btn btn-primary" onclick="addLine()">
                        <i class="glyphicon glyphicon-plus"></i>  Add Product
                    </a>
                </div>
                <p></p>
                <div class="top-buffer"></div>
                <table class="table table-bordered table-striped table-condensed">
                    <thead>
                        <tr>
                            <th>Product</th>
                            <th>Application Rate</th>
                            <th>Quantity</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.Lines)
                        {
                            <tr>
                                <td>@item.ProductTarget.Name</td>
                                <td>@item.ApplicationRate</td>
                                <td>@item.Quantity</td>
                                <td>
                                    <a id="editLineModal" data-toggle="modal" asp-action="EditLine" asp-route-id="@item.Id" data-target="#modal-action-edit-line" class="btn btn-info">
                                        <i class="glyphicon glyphicon-pencil"></i>  Edit
                                    </a>
                                    <a id="deleteLineModal" data-toggle="modal" asp-action="DeleteLine" asp-route-id="@item.Id" data-target="#modal-action-delete-line" class="btn btn-danger">
                                        <i class="glyphicon glyphicon-trash"></i>  Delete
                                    </a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <div>
            <label>three</label>
        </div>
        <div>
            <label>Four</label>
        </div>
        <div>
            <label>Five</label>
        </div>
    </div>

    <div>
        <a id="createAddProductMixModal" class="btn btn-danger" style="margin-right:5px; width:45%" onclick="BackClicked()">
            <i class="glyphicon glyphicon-arrow-left"></i>  Back
        </a>
        <a id="createAddProductMixModal" class="btn btn-primary" style="width:45%; float:right" onclick="NextClicked()">
            <i class="glyphicon glyphicon-arrow-right"></i>  Next
        </a>
    </div>
</form>

@Html.Partial("_Modal", new BootstrapModel { ID = "modal-action-add-line", AreaLabeledId = "modal-action-user-label", Size = ModalSize.Medium })
@Html.Partial("_Modal", new BootstrapModel { ID = "modal-action-edit-line", AreaLabeledId = "modal-action-user-label", Size = ModalSize.Medium })
@Html.Partial("_Modal", new BootstrapModel { ID = "modal-action-delete-line", AreaLabeledId = "modal-action-user-label", Size = ModalSize.Medium })

@section scripts{
    <script>
        $(document).ready(function () {
            $("#tabstrip").kendoTabStrip({
                animation: {
                    open: {
                        effects: "fadeIn"
                    }
                }
            });

            $("#company").kendoDropDownList({
                suggest: false,
                change: OnCompanyChange
            });

            $("#employee").kendoDropDownList({
                dataValueField: "value",
                dataTextField: "text",
                suggest: false
            });

            $("#uiAreaTreated").kendoNumericTextBox();

            $("#uiDateApplied").kendoDatePicker({
                format: "dd/MM/yyyy"
            });

            $("#uiDateApplied").bind("focus", function () {
                $(this).data("kendoDatePicker").open();
            });

            $('#uiForm').on('submit', function (e) {
                e.preventDefault();

                var formData = $(this).serialize();
                formData += "&Employee=" + $("#employee").data("kendoDropDownList").value();

                $.post('/Spraying/AddLine', formData, function (response) {
                    $("#modal-action-add-line").html(response);
                    $('#modal-action-add-line').modal('toggle'); 
                });
            });

            $("#uiDateApplied").data("kendoDatePicker").value(new Date());

        });

        function ImageClick(checkbox) {
            checkbox.checked = !checkbox.checked;
        }

        function OnCompanyChange() {
            var tradingEntity = $("#company").data("kendoDropDownList").value();
            var combo = $("#employee").data("kendoDropDownList");
            var dataSource = new kendo.data.DataSource({
                data: [{ value: -1, text: "Select Employee..." }]
            });
            combo.setDataSource(dataSource);
            combo.select(0);

            $.ajax({
                url: "/api/Data/GetEmployee",
                data: JSON.stringify({ "Id": tradingEntity }),
                type: 'POST',
                traditional: true,
                contentType: 'application/json',
                success: function (result) {
                    for (i = 0; i < result.length; i++) {
                        combo.dataSource.add({ value: result[i].id, text: result[i].firstName + " " + result[i].lastName });
                    }
                }
            });
        }

        function BackClicked() {
            var index = $("#tabstrip").data("kendoTabStrip").select().index();

            if (index === 0) {
                myconfirm("Quit Pesticide Application?", "Quit?").then(function () {
                    window.location.href = "../Spraying";
                });

                return;
            }

            $("#tabstrip").data("kendoTabStrip").select(--index);
        }

        function NextClicked() {
            var index = $("#tabstrip").data("kendoTabStrip").select().index();

            if (index === 5) {
                //last page check if want to submit
                return;
            }

            $("#tabstrip").data("kendoTabStrip").select(++index);
        }

        function myconfirm(content, title) {
            return $("<div></div>").kendoConfirm({
                title: title,
                content: content
            }).data("kendoConfirm").open().result;
        }

        function addLine() {
            
            $("#uiForm").submit();
        }

    </script>

}
