@model string

<form asp-action="SendAccountDetails" role="form">
    @Html.Partial("_ModalHeader", new ModalHeader { Heading = "Send Account Details" })

    <div class="modal-body form-horizontal">
        Send account details to @Model?
    </div>
    @Html.Partial("_ModalFooter", new ModalFooter { SubmitButtonText = "Send" })
</form>

